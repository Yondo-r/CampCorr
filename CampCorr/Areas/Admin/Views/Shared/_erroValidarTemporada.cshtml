@{
    bool existemErros = false;
    if (TempData["erros"] != null)
    {
        existemErros = true;
    };

}
@if (existemErros)
{
    <h4>Lista Erros</h4>
    <div class="form-group">
        <span class="text-danger"> 
            @Html.Raw(TempData["erros"])
        </span>
    </div>
}
@*@TempData["erros"].ToString()*@